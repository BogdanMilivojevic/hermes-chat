{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-indent" : ["error", 2],
    "react/prop-types": [0]
  },
  "settings": {
    "react": {
      "version": "18.2"
    }
  }
}